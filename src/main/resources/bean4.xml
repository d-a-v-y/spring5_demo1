<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="emp" class="com.atguigu.spring.bean.Emp">
        <property name="ename" value="ztf"></property>
        <property name="gender" value="male"></property>
        <property name="dept" ref="dept"></property>
        <property name="dept.dname" value="technology"></property>
    </bean>
    <bean id="dept" class="com.atguigu.spring.bean.Dept">
        <property name="dname" value="money"></property>
    </bean>
</beans>